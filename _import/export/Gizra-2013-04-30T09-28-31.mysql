INSERT INTO `locales_source` VALUES ('3618','/he/admin/build/views/edit/seminars?destination=seminars','default','Live preview','6.16',''),('3619','/he/admin/build/views/edit/seminars?destination=seminars','default','If you install the advanced help module from !href, Views will provide more and better help. <a href="@hide">Hide this message.</a>','6.16',''),('3620','/he/admin/build/views/ajax/display/seminars/page_1/header','default','Override','6.16',''),('3621','/he/admin/build/views/ajax/display/seminars/page_1/header','default','Status: using default values.','6.16',''),('3622','/he/admin/build/views/ajax/display/seminars/page_1/header','default','Update default display','6.16',''),('3623','/he/admin/build/views/ajax/display/seminars/page_1/header','default','Display even if view has no result','6.16',''),('3624','/he/admin/build/views/ajax/display/seminars/page_1/header','default','Text to display at the top of the view. May contain an explanation or links or whatever you like. Optional.','6.16',''),('3625','seminars:page_1:header','views','משיחות עם חברי הקהילה הבנו שאחד הדברים החסרים הם סדנאות קצרות ומרוכזות על נושאים ספציפיים. היתרון המשמעותי במפגשים כאלו, לעומת מפגשים קהילתיים כפי שאירגנו בפעמים האחרונות, הוא שלמשתתפים יש הזדמנות לתרגל את הנלמד, ולא פחות חשוב - יש מישהו לשאול כשנתקלים בבעיות.\n\nכל סדנא מתפרשת על שני מפגשים שאורך כל אחד מהם 3 שעות (סה"כ 6 שעות)\n\nמחיר סדנא בודדת: 650 ש"ח\nמחיר שתי סדנאות: 1000 ש"ח\n\nתאריך ושעה לגבי כל סדנא תינתן לאחר שיירשם מינימום של אנשים. \nעל המשתתפים להגיע עם המחשב הנייד שלהם.\nהסדנאות ינתנו במשרדי חברת גיזרה, ברחוב ברנר בתל-אביב.\nרוצים להציע סדנאות נוספות? <a href=seminars-contact-us>פנו אלינו</a>\n\n\n','1','');
INSERT INTO `locales_source` VALUES ('3626','/he/admin/build/views/ajax/display/seminars/page_1/header','default','משיחות עם חברי הקהילה הבנו שאחד הדברים החסרים הם סדנאות קצרות ומרוכזות על נושאים ספציפיים. היתרון המשמעותי במפגשים כאלו, לעומת מפגשים קהילתיים כפי שאירגנו בפעמים האחרונות, הוא שלמשתתפים יש הזדמנות לתרגל את הנלמד, ולא פחות חשוב - יש מישהו לשאול כשנתקלים בבעיות.\n\nכל סדנא מתפרשת על שני מפגשים שאורך כל אחד מהם 3 שעות (סה"כ 6 שעות)\n\nמחיר סדנא בודדת: 650 ש"ח\nמחיר שתי סדנאות: 1000 ש"ח\n\nתאריך ושעה לגבי כל סדנא תינתן לאחר שיירשם מינימום של אנשים. \nעל המשתתפים להגיע עם המחשב הנייד שלהם.\nהסדנאות ינתנו במשרדי חברת גיזרה, ברחוב ברנר בתל-אביב.\nרוצים להציע סדנאות נוספות? <a href=seminars-contact-us>פנו אלינו</a>\n\n\n','6.19',''),('3627','/he/admin/build/views/ajax/preview/seminars','default','Query','6.16',''),('3628','/he/admin/build/views/ajax/preview/seminars','default','This display has no path.','6.16',''),('3629','/he/admin/build/views/ajax/preview/seminars','default','Query build time','6.16',''),('3630','/he/admin/build/views/ajax/preview/seminars','default','@time ms','6.16',''),('3631','/he/admin/build/views/ajax/preview/seminars','default','Query execute time','6.16',''),('3632','/he/admin/build/views/ajax/preview/seminars','default','View render time','6.16',''),('3633','/he/admin/build/views/edit/seminars?destination=seminars','default','The view %name has been saved.','6.16',''),('3634','type:poll:name','nodetype','Poll','1',''),('3635','type:poll:title','nodetype','Question','1',''),('3636','type:poll:description','nodetype','A <em>poll</em> is a question with a set of possible responses. A <em>poll</em>, once created, automatically provides a simple running count of the number of votes received for each response.','1','');
