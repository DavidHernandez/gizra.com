DROP TABLE IF EXISTS `actions`;
CREATE TABLE `actions` (   `aid` varchar(255) NOT NULL default '0',   `type` varchar(32) NOT NULL default '',   `callback` varchar(255) NOT NULL default '',   `parameters` longblob NOT NULL COMMENT 'Parameters to be passed to the callback function.',   `label` varchar(255) NOT NULL default '0',   PRIMARY KEY  (`aid`) ) ENGINE=MyISAM DEFAULT CHARSET=utf8;
