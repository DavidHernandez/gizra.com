DROP TABLE IF EXISTS `fontyourface_tag`;
CREATE TABLE `fontyourface_tag` (   `tid` int(10) unsigned NOT NULL auto_increment COMMENT 'Primary Key: Unique tag ID.',   `name` varchar(255) NOT NULL default '' COMMENT 'The font name.',   `type` varchar(10) NOT NULL default 'tag' COMMENT 'Indicator that describes the type of tag (term, subset, etc).',   PRIMARY KEY  (`tid`) ) ENGINE=MyISAM AUTO_INCREMENT=17 DEFAULT CHARSET=utf8 COMMENT='Font tag information.' AUTO_INCREMENT=17;
INSERT INTO `fontyourface_tag` VALUES ('9','latin','subset'),('10','latin-ext','subset'),('11','greek','subset'),('12','cyrillic','subset'),('13','cyrillic-ext','subset'),('14','khmer','subset'),('15','greek-ext','subset'),('16','vietnamese','subset');
